<!DOCTYPE html>
<html lang="en">
    <head>

        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="author" content="">

        <title>{block #titles}{/block} | Caloris Admin</title>

        <link href="{$baseUri}/css/bootstrap.min.css" rel="stylesheet">
        <link href="{$baseUri}/css/metisMenu.min.css" rel="stylesheet">
        <link href="{$baseUri}/css/timeline.css" rel="stylesheet">
        <link href="{$baseUri}/css/sb-admin-2.css" rel="stylesheet">
        <link href="{$baseUri}/css/morris.css" rel="stylesheet">
        <link href="{$baseUri}/css/font-awesome.min.css" rel="stylesheet" type="text/css">
        <link href="{$baseUri}/css/summernote.css" rel="stylesheet" type="text/css">
        <link href="{$baseUri}/css/style_admin.css" rel="stylesheet" type="text/css">

        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
            <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
    </head>
    <body>
        {if $signed}
            <div id="wrapper">
                <nav class="navbar navbar-default" role="navigation" style="margin-bottom: 0">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="sr-only">Toggle</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" n:href=":Admin:Homepage:default"><img src="{$baseUri}/images/caloris-logo-mini.jpg" /></a>
                    </div>

                    <ul class="nav navbar-top-links navbar-right">
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#">
                                <i class="fa fa-user fa-fw"></i>  <i class="fa fa-caret-down"></i>
                            </a>
                            <ul class="dropdown-menu dropdown-user">
                                <li><a n:href=":Admin:Profile:default"><i class="fa fa-gear fa-fw"></i> {_admin.navigation.myprofile}</a>
                                </li>
                                <li class="divider"></li>
                                <li><a n:href=":Admin:Sign:out"><i class="fa fa-sign-out fa-fw"></i> {_admin.navigation.logout}</a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                    <div class="navbar-default sidebar" role="navigation">
                        <div class="sidebar-nav navbar-collapse">
                            <ul class="nav" id="side-menu">
                                <li><a n:href=":Admin:Homepage:default"><i class="fa fa-sitemap fa-fw"></i> {_admin.navigation.homepage}</a></li>
                                <li><a n:href=":Admin:Contacts:default"><i class="fa fa-sitemap fa-fw"></i> {_admin.navigation.contacts}</a></li>
                                <li><a n:href=":Admin:Gallery:default"><i class="fa fa-shopping-cart fa-fw"></i> {_admin.navigation.gallery}</a></li>
                                <li><a n:href=":Admin:Pages:default"><i class="fa fa-files-o fa-fw"></i> {_admin.navigation.pages}</a></li>
                                <li><a n:href=":Admin:Members:default"><i class="fa fa-users fa-fw"></i> {_admin.navigation.members}</a></li>
                                <li><a n:href=":Admin:Files:default"><i class="fa fa-floppy-o fa-fw"></i> {_admin.navigation.files}</a></li>
                                <li><a n:href=":Admin:Categories:default"><i class="fa fa-sitemap fa-fw"></i> {_admin.navigation.categories}</a></li>
                                <li><a href="#"><i class="fa fa-shopping-cart fa-fw"></i> Store</a>
                                    <ul class="nav nav-second-level collapse" aria-expanded="false">
                                        <li><a n:href=":Admin:Catalogue:default"> {_admin.navigation.catalogue}</a></li>
                                        <li><a n:href=":Admin:Orders:default"> {_dictionary.main.Orders}</a></li>
                                        <li><a n:href=":Admin:StoreSettings:default"> {_dictionary.main.Settings}</a></li>
                                        <li><a n:href=":Admin:Catalogue:brands">{_dictionary.main.Brands}</a></li>
                                        <li><a n:href=":Admin:Catalogue:parametres">{_dictionary.main.Parametres}</a></li>
                                    </ul>
                                </li>
                                <li><a n:href=":Admin:Settings:default"> {_dictionary.main.Settings}</a></li>
                            </ul>
                        </div>
                    </div>
                </nav>
                <div id="page-wrapper">

                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-lg-12">
                                {include content}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        {else}
            <div class="container">
                <div class="row">
                    <div class="col-md-4 col-md-offset-4">
                        <div class="login-panel panel panel-default">
                            {include content}
                        </div>
                    </div>
                </div>
            </div>
        {/if}
        <script src="{$baseUri}/js/jquery.js"></script>
        <script src="{$baseUri}/js/bootstrap.min.js"></script>
        <script src="{$baseUri}/js/metisMenu.min.js"></script>
        <script src="{$baseUri}/js/summernote.min.js"></script>
        <script src="{$baseUri}/js/jquery.autocomplete.js"></script>
        <script src="{$baseUri}/js/dropzone.js"></script>
        <script src="{$baseUri}/js/netteForms.js" type="text/javascript"></script>
        <script src="{$baseUri}/js/nette.ajax.js" type="text/javascript"></script>
        <script src="{$baseUri}/js/sb-admin-2.js"></script>

    </body>
</html>

